  // Add your type properties here
}

<% } %>
<% if (createEnum) { %>
/**
 * <%= name.split('-').map(part => part.charAt(0).toUpperCase() + part.slice(1)).join('') %>Status enum
 */
export enum <%= name.split('-').map(part => part.charAt(0).toUpperCase() + part.slice(1)).join('') %>Status {
  ACTIVE = 'active',
  INACTIVE = 'inactive',
  PENDING = 'pending',
}
<% } %>
